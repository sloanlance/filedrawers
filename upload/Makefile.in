CC= @CC@
INCPATH=    -I../libcgi
CFLAGS=     -Wall -Wstrict-prototypes -g ${INCPATH}
LIBS=       -L../libcgi -lcgi
OBJ= tpupload.o

URL_ERROR_PAGE=@error_url@

INSTALL=    @INSTALL@

all:    tpupload

tpupload.o: tpupload.c
	    ${CC} ${CFLAGS} \
	    -D_URL_ERROR_PAGE=\"${URL_ERROR_PAGE}\" \
	    -c tpupload.c

tpupload:   ${OBJ}
	    ${CC} ${CFLAGS} -o upload.cgi ${OBJ}  ${LIBS}

clean :
	    rm -f *.o a.out core *.core
	    rm -f upload.cgi
